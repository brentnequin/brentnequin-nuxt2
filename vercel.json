{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {}
        }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "api/index.py"
        }
    ]
} 